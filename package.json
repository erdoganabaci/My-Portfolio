{
  "name": "erdogan-portfolio",
  "version": "0.1.0",
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "axios": "^1.7.9",
    "colorthief": "^2.4.0",
    "gh-pages": "^6.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-easy-emoji": "^1.8.1",
    "react-headroom": "^3.2.1",
    "react-lottie": "^1.2.4",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^7.1.5",
    "react-scripts": "^5.0.1",
    "react-twitter-embed": "^4.0.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "start": "node fetch.js && react-scripts start",
    "build": "node fetch.js && react-scripts build",
    "deploy": "gh-pages -b master -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"./**/*.{js,css,json}\""
  },
  "eslintConfig": {
    "extends": "react-app",
    "ignorePatterns": [
      "src/portfolio.js",
      "src/components/githubProfileCard/GithubProfileCard.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "dotenv": "^16.4.1",
    "prettier": "^3.2.5"
  }
}
